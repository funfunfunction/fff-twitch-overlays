{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint:check": "eslint -c .eslintrc.js --ext .ts src",
    "lint:fix": "eslint --fix -c .eslintrc.js --ext .ts src",
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "logs": "firebase functions:log",
    "check-all": "npm-run-all build lint:check style:check",
    "style:check": "prettier --check \"src/**/*.ts\"",
    "style:fix": "prettier --write \"src/**/*.ts\""
  },
  "engines": {
    "node": "8"
  },
  "prettier": {
    "semi": false
  },
  "dependencies": {
    "@google-cloud/bigquery": "^4.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/node-fetch": "^2.5.7",
    "@types/query-string": "^6.3.0",
    "@types/simple-oauth2": "^2.5.2",
    "@types/tmi.js": "^1.4.0",
    "cookie-parser": "^1.4.5",
    "crypto": "^1.0.1",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.1",
    "firebase-tools": "^8.1.1",
    "is-string": "^1.0.5",
    "node-fetch": "^2.6.0",
    "query-string": "^6.12.1",
    "simple-oauth2": "^3.4.0",
    "tmi.js": "^1.5.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "firebase-functions-test": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "typescript": "^3.2.2"
  },
  "private": true,
  "main": "lib/index.js"
}
